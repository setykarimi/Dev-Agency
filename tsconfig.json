{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "Components/*": ["./src/components/*"],
      "Pages/*": ["./src/pages/*"],
      "Layout/*": ["./src/layout/*"],
      "Images/*":[ "./src/assets/images/*"],
      "Data/*": ["./src/assets/data/*"],
    },
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}